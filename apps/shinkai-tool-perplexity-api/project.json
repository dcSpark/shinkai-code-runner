{
  "name": "@shinkai_protocol/shinkai-tool-perplexity-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/shinkai-tool-perplexity-api/src",
  "projectType": "library",
  "tags": [
    "tool"
  ],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "tsc",
        "outputPath": "dist/apps/shinkai-tool-perplexity-api",
        "main": "apps/shinkai-tool-perplexity-api/src/index.ts",
        "tsConfig": "apps/shinkai-tool-perplexity-api/tsconfig.app.json",
        "webpackConfig": "apps/shinkai-tool-perplexity-api/webpack.config.ts"
      },
      "configurations": {
        "development": {
        },
        "production": {
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/shinkai-tool-perplexity-api/**/*.ts",
          "apps/shinkai-tool-perplexity-api/package.json"
        ]
      }
    }
  }
}
